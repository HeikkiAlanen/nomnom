{
  "name": "nomnom",
  "description": "Option parser with generated usage and commands",
  "version": "1.8.0",
  "author": "Heather Arthur <fayearthur@gmail.com>",
  "scripts": {
    "test": "./node_modules/.bin/nodeunit test",
    "coverage": "istanbul cover ./node_modules/nodeunit/bin/nodeunit test",
	"coverageTravisCI": "istanbul cover --report lcovonly ./node_modules/nodeunit/bin/nodeunit -- test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/harthur/nomnom.git"
  },
  "main": "./nomnom",
  "keywords": [
    "arguments",
    "option parser",
    "command line",
    "options",
    "parser"
  ],
  "dependencies": {
    "underscore": "~1.6.0",
    "chalk": "~0.4.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "nodeunit": "~0.7.4"
  }
}
